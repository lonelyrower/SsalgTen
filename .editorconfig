# Editor configuration for SsalgTen
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2

[*.ts]
indent_size = 2

[*.tsx]
indent_size = 2

[*.js]
indent_size = 2

[*.jsx]
indent_size = 2

[*.json]
indent_size = 2

[*.css]
indent_size = 2

[*.scss]
indent_size = 2

[*.md]
trim_trailing_whitespace = false

